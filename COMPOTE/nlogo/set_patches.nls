to set-patches
  ;;lire les fichiers csv 
  ask patches[
    set Variety one-of [1 2 3]
    set pcolor ifelse-value (Variety = 2 )[red][ifelse-value (Variety = 3) [green][blue]]
    set Sensibility ifelse-value (Variety = 2) [Svar2][ifelse-value (Variety = 3) [Svar3][Svar1]] ;TODO: table/parameters of Sensibility and Quality in fonction of Variety
    set Quality 1 - Sensibility
    set Production 0.1 
    set Infest 0
  ]
  
end
